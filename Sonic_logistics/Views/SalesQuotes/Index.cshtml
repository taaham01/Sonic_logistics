                @Html.DisplayFor(modelItem => item.Uom)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PriceList)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UnitSellingPrice)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TotalPrice)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Discription)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ProductAvalibility)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CreateDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ExpireDate)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.SqId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.SqId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.SqId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
