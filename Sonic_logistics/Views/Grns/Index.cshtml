@model IEnumerable<Sonic_logistics.Models.Grn>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.GrnDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SupplierName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PoId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PoDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Warehouse)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ProductId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ProductName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.BatchNo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ApprovedWarehouse)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.UnapprovedWarehouse)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.GrnDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SupplierName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PoId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PoDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Warehouse)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ProductId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ProductName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.BatchNo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ApprovedWarehouse)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UnapprovedWarehouse)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.GrnId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.GrnId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.GrnId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
